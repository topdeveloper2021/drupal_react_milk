    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "tests/behat/features/bootstrap/FeatureContext.php"
        ]
    },
    "scripts": {
        "analyze": [
            "php -d memory_limit=-1 drush upgrade_status:analyze --all"
        ],
        "update-vendor-dir": [
            "php -d memory_limit=-1 `which composer` update -W --optimize-autoloader --prefer-dist",
            "cd ./web/libraries/bootstrap/dist && cp -R * ..",
            "vendor/bin/phpcs --config-set installed_paths web/modules/contrib/coder/coder_sniffer"
        ],
        "install-vendor-dir": [
            "php -d memory_limit=-1 `which composer` install --optimize-autoloader --prefer-dist",
            "cd ./web/libraries/bootstrap/dist && cp -R * ..",
            "vendor/bin/phpcs --config-set installed_paths web/modules/contrib/coder/coder_sniffer"
        ],
        "install-codestandards": [
            "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
        ],
        "build-assets": [
            "@install-vendor-dir",
            "@prepare-for-pantheon",
            "npm install && npm run build"
        ],
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": [
            "vendor/bin/phpcs --config-set installed_paths vendor/drupal/coder/coder_sniffer",
            "vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
            "vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/custom",
            "vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
            "vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/custom"
        ],
        "code-fix": [
            "vendor/bin/phpcs --config-set installed_paths vendor/drupal/coder/coder_sniffer",
            "`which phpcbf` -vp --standard=Drupal,DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md web/modules/custom"
        ],
        "unit-test": "echo 'No unit test step defined.'",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
        "post-install-cmd": [
            "bash scripts/setup-precommit.sh",
            "cp -R web/libraries/bootstrap/dist/* web/libraries/bootstrap",
            "@drupal-scaffold",
            "@install-codestandards",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "bash scripts/setup-precommit.sh",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@install-codestandards"
        ],
        "post-create-project-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-commit": [
            "composer validate --strict",
            "composer update --with-dependencies",
            "git add composer.lock",
            "@code-fix",
            "@code-sniff"
        ]
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset",
            "drupal-drush",
            "drupal-theme",
            "drupal-profile",
            "drupal-module",
            "drupal-core"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/private/scripts/quicksilver/{$name}/": [
                "type:quicksilver-script"
            ],
            "web/libraries/{$name}": [
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "phpcodesniffer-search-depth": 5,
        "build-env": {
            "install-cms": [
                "drush site-install standard --account-mail={account-mail} --account-name={account-name} --account-pass={account-pass} --site-mail={site-mail} --site-name={site-name} --yes"
            ],
            "export-configuration": "drush config-export --yes"
        },
        "drupal-scaffold": {
            "source": "https://raw.githubusercontent.com/pantheon-systems/drops-8-scaffolding/{version}/{path}",
            "includes": [
                "sites/default/default.services.pantheon.preproduction.yml",
                "sites/default/settings.pantheon.php"
            ],
            "excludes": [
                ".csslintrc",
                ".editorconfig",
                ".eslintignore",
                ".eslintrc.json",
                ".htaccess",
                "web.config"
            ],
            "allowed-packages": [
                "drupal/core"
            ],
            "overwrite": true,
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[web-root]/sites/default/settings.local.php": "config/settings.local.php",
                "[web-root]/sites/default/settings.pantheon.php": "config/settings.pantheon.php"
            }
        },
        "patches-file": "composer.patches.json",
        "enable-patching": true,
        "composer-exit-on-patch-failure": true
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0,
        "fxp-asset": {
            "enabled": true,
            "vcs-driver-options": {
                "github-no-api": true
            }
        },
        "platform": {
            "php": "7.4"
        }
    }
}
